{
  "manifest_version": 2,
  "name": "Clipboard- Manager Extension",
  "version": "1.0.1",
  "description": "Clipboard- Manager Extension",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "PoP-up"
  },
  "permissions": ["activeTab", "tabs", "clipboardRead", "clipboardWrite"],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  }
}
